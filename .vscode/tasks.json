{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "go: build (debug)",
			"type": "shell",
			"command": "go",
			"args": [
				"build",
				"-gcflags=all=-N -l",
				"-o",
				"${workspaceFolder}/bin/__debug_bin.exe"
			],
			"options": {
				"cwd": "${workspaceFolder}/"
			},
			"problemMatcher": [
				"$go"
			]
		}
	]
}